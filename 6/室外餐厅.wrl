PROTO image[
field SFVec3f position 0 0 0
field SFVec3f scale 1 1 1 
field MFString url ""
]
{
Transform{
translation	IS position
children [
	Billboard {
		children [
			Transform {
			scale IS scale
			children[
				Shape {
					appearance Appearance {
						material Material {}
						texture	ImageTexture {
						url	IS url
						}
					}
					geometry IndexedFaceSet	{
						coord Coordinate {
							point [-0.5 0 0
								    0.5 0 0
								    0.5 1 0
								   -0.5 1 0
							] 
						}
						coordIndex [0 1 2 3]
					}
				}
			]
		}
		]
	}
]
}
}

